{
  "name": "redpanda",
  "version": "0.0.1-alpha",
  "description": "A lightweight Javascript library for executing, mapping, chaining data with your API calls",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node build/scripts/compile",
    "build": "npm run clean && cross-env NODE_ENV=production npm run compile",
    "test": "npm test",
    "start": "cross-env NODE_ENV=development node build/scripts/start",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hungluu2106/redpanda.git"
  },
  "keywords": [
    "red",
    "panda",
    "redpanda",
    "api",
    "call",
    "map",
    "data",
    "chain",
    "mock"
  ],
  "author": "Hung Luu <hungluu.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hungluu2106/redpanda/issues"
  },
  "homepage": "https://github.com/hungluu2106/redpanda#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.1",
    "dirty-chai": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "json3": "^3.3.2",
    "mocha": "^3.5.0",
    "rimraf": "^2.6.1",
    "webpack": "^3.5.4"
  }
