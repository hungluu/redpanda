{
  "name": "redpanda",
  "version": "0.0.6",
  "description": "A lightweight Javascript library for executing, mapping, chaining data with your API calls",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node build/scripts/compile",
    "build": "npm run clean && cross-env NODE_ENV=production npm run compile",
    "start": "cross-env NODE_ENV=development node build/scripts/start",
    "fast-test": "cross-env NODE_ENV=test karma start build/karma.config",
    "test:watch": "npm run fast-test -- --watch",
    "test": "npm run lint & npm run fast-test",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "doc": "documentation build src/RedPanda.js -f md -o APIDOC.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hungluu2106/redpanda.git"
  },
  "keywords": [
    "red",
    "panda",
    "redpanda",
    "api",
    "call",
    "map",
    "data",
    "chain",
    "mock",
    "ajax",
    "json",
    "fetch",
    "advanced"
  ],
  "author": "Hung Luu <hungluu.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hungluu2106/redpanda/issues"
  },
  "homepage": "https://github.com/hungluu2106/redpanda#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.1",
    "chai-as-promised": "^6.0.0",
    "compression": "^1.7.0",
    "cross-env": "^5.0.5",
    "dirty-chai": "^2.0.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.15.4",
    "figures": "^2.0.0",
    "fs-extra": "^4.0.1",
    "karma": "^1.7.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack-with-fast-source-maps": "^1.10.0",
    "kind-of": "^5.0.2",
    "mocha": "^3.5.0",
    "node-fetch": "^1.7.2",
    "node-noop": "^1.0.0",
    "promise": "^8.0.1",
    "rimraf": "^2.6.1",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0",
    "webpack": "^3.5.4",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "whatwg-fetch": "^2.0.3"
  }
}
